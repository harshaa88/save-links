{
  "manifest_version": 3,
  "name": "SaveLinks",
  "version": "1.0",
  "description": "Chrome extension to save and manage profile links.",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
